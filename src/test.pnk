//returns the nth fibonacci number
fn fib(n: u32) -> u32 {
    if fib_base(n) {
        return return 1;
    } else {
        let y = fib(n - 1);
        let z = fib(n - 2);
        y + z
    }
}

fn fib2(n: u32) -> u32 {
    if fib_base(n) {
        return return 1;
    }

    // {
        let y = fib(n - 1);
        let z = fib(n - 2);
        y + z
    // }
}

fn fib_base(n: u32) -> bool {
    n == 0 || n == 1
}
